  {
    "manifest_version": 3,
    "name": "Hello World",
    "description": "This extension to test html injection",
    "version": "1.0",
    "content_scripts": [{
        "run_at": "document_end",
        "matches": [
            "https://*/*",
            "http://*/*"
        ],
        "js": ["content.js"]
    }],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
  }