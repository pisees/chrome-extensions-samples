  {
    "version": "1.1",
    "manifest_version": 3,
    "name": "Observability Copilot",
    "description": "Ask questions about avaialbility metrics",
    "content_scripts": [{
        "matches": [
            "https://*/*",
            "http://*/*"
        ],
        "js": ["content.js"]
    }],
    "background": {
      "service_worker": "background.js"
    },
    "action": {},
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "web_accessible_resources": [
      { 
        "resources": [
          "popup.html",
          "https://www.bing.com/chat"
        ], 
        "matches": ["*://*/*"]
      }    
    ]
}

