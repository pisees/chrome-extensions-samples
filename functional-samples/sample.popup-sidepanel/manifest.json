  {
    "manifest_version": 2,
    "name": "Hello World",
    "description": "This extension to test html injection",
    "version": "1.0",
    "content_scripts": [{
        "run_at": "document_end",
        "matches": [
            "https://*/*",
            "http://*/*"
        ],
        "js": ["content.js"]
    }],
    "browser_action": {
    },
    "background": {
        "scripts":["background.js"],
    "permissions": [
        "activeTab"
    ],
    "web_accessible_resources": ["popup.html"]
  }
}